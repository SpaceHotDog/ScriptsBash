var match = {
    $match: {
        "fechaIngreso": { "$gte" : ISODate("2019-02-01T00:00:00.000Z"),
        		  "$lte" : ISODate("2019-02-15T23:59:59.999Z")},
        "idUsuario" : "961",
        "puntoDeInsercion" : "wsFrontera"}
}

var group = {
    $group: {
        _id : "$idUsuario",
        count: { $sum: 1 }
    }
};

db.getCollection('Mensajes').aggregate([match, group]);
